<?xml version="1.0" encoding="utf-8" ?>
<ContentView
    x:Class="MealMate.View.NavBar"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Grid BackgroundColor="{StaticResource CustomMoerkeGraa}" ColumnDefinitions="*,*,*">
        <Grid Grid.Column="0">
            <ImageButton
                Padding="25"
                Pressed="hjemmesideSkearm_knap"
                Source="hjem_ikon.png" />
        </Grid>

        <Grid Grid.Column="1">
            <ImageButton
                Padding="25"
                Pressed="tilfoejMad_knap"
                Source="tilfoej_ikon.png" />
        </Grid>

        <Grid Grid.Column="2">
            <ImageButton
                Padding="25"
                Pressed="profilside_knap"
                Source="profil_ikon.png" />
        </Grid>
    </Grid>
</ContentView>